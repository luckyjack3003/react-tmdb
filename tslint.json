{
  "extends": ["tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": [false],
    "object-literal-sort-keys": [false],
    "allowArrowFunctions":true,
    "ignoreDOMComponents":true,
    "jsx-no-lambda": false
  }
}
